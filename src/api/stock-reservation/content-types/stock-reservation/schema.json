{
  "kind": "collectionType",
  "collectionName": "stock_reservations",
  "info": {
    "singularName": "stock-reservation",
    "pluralName": "stock-reservations",
    "displayName": "Stock Reservation"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "reservationId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "sessionId": {
      "type": "string",
      "required": true
    },
    "userId": {
      "type": "string"
    },
    "variant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-variant.product-variant"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "status": {
      "type": "enumeration",
      "required": true,
      "default": "reserved",
      "enum": [
        "reserved",
        "confirmed",
        "expired",
        "cancelled"
      ]
    },
    "expiresAt": {
      "type": "datetime",
      "required": true
    },
    "confirmedAt": {
      "type": "datetime"
    },
    "cancelledAt": {
      "type": "datetime"
    },
    "metadata": {
      "type": "json"
    }
  }
}
