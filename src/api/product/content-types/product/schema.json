{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 200,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "short_description": {
      "type": "text",
      "maxLength": 300
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "original_price": {
      "type": "decimal",
      "min": 0
    },
    "stock": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0
    },
    "sku": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "brand": {
      "type": "string",
      "maxLength": 100
    },
    "model": {
      "type": "string",
      "maxLength": 100
    },
    "store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "Media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "freeShipping": {
      "type": "boolean"
    },
    "hasVariants": {
      "type": "boolean"
    },
    "rating": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 5
    },
    "brand": {
      "type": "string",
      "maxLength": 100
    },
    "subcategory": {
      "type": "string"
    },
    "weight": {
      "type": "decimal",
      "min": 0
    },
    "dimensions": {
      "type": "json"
    },
    "color": {
      "type": "string",
      "maxLength": 50
    },
    "size": {
      "type": "string",
      "maxLength": 20
    },
    "material": {
      "type": "string",
      "maxLength": 100
    },
    "power": {
      "type": "string",
      "maxLength": 50
    },
    "voltage": {
      "type": "string",
      "maxLength": 20
    },
    "warranty": {
      "type": "string",
      "maxLength": 200
    },
    "shipping_info": {
      "type": "text"
    },
    "return_policy": {
      "type": "text"
    },
    "specifications": {
      "type": "json"
    },
    "free_shipping": {
      "type": "boolean",
      "default": false
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "originalPrice": {
      "type": "decimal",
      "min": 0
    },
    "discountPercentage": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 100
    },
    "reviewCount": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "maxQuantity": {
      "type": "integer",
      "default": 10,
      "min": 1
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant"
    },
    "activityProductViews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-product-view.activity-product-view",
      "mappedBy": "product"
    }
  }
}
