{
  "kind": "collectionType",
  "collectionName": "user_behaviors",
  "info": {
    "singularName": "user-behavior",
    "pluralName": "user-behaviors",
    "displayName": "User Behavior",
    "description": "Track user interactions and behavior patterns"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "userId": {
      "type": "string",
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    },
    "action": {
      "type": "enumeration",
      "enum": [
        "view",
        "add_to_cart",
        "remove_from_cart",
        "purchase",
        "favorite",
        "search",
        "category_view"
      ],
      "required": true
    },
    "context": {
      "type": "string",
      "required": true
    },
    "sessionId": {
      "type": "string"
    },
    "timestamp": {
      "type": "datetime",
      "required": true
    },
    "metadata": {
      "type": "json"
    }
  }
} 